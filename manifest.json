{
    "modulesContent": {
        "$edgeAgent": {
            "properties.desired": {
                "modules": {
                    "interface1": {
                        "settings": {
                            "image": "simulated-win-sensor:1.0",
                            "createOptions": ""
                        },
                        "type": "docker",
                        "version": "1.0",
                        "env": {
                            "ASSET": {
                                "value": "olympus"
                            },
                            "DEVICE": {
                                "value": "OL-GEN-INST"
                            }  
                        },
                        "status": "running",
                        "restartPolicy": "always"
                    },
                    "interface2": {
                        "settings": {
                            "image": "simulated-win-sensor:1.0",
                            "createOptions": ""
                        },
                        "type": "docker",
                        "version": "1.0",
                        "env": {
                            "ASSET": {
                                "value": "camelot"
                            },
                            "DEVICE": {
                                "value": "CA-GEN-INST"
                            }
                        },
                        "status": "running",
                        "restartPolicy": "always"
                    }
                },
                "runtime": {
                    "settings": {
                        "minDockerVersion": "v1.25"
                    },
                    "type": "docker"
                },
                "schemaVersion": "1.0",
                "systemModules": {
                    "edgeAgent": {
                        "settings": {
                            "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
                            "createOptions": ""
                        },
                        "type": "docker"
                    },
                    "edgeHub": {
                        "settings": {
                            "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
                            "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"443/tcp\":[{\"HostPort\":\"443\"}],\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}]}}}"
                        },
                        "type": "docker",
                        "status": "running",
                        "restartPolicy": "always"
                    }
                }
            }
        },
        "$edgeHub": {
            "properties.desired": {
                "routes": {
                    "interface1": "FROM /messages/modules/interface1/outputs/* INTO $upstream",
                    "interface2": "FROM /messages/modules/interface2/outputs/* INTO $upstream",
                    "upstream": "FROM /messages/* INTO $upstream"
                },
                "schemaVersion": "1.0",
                "storeAndForwardConfiguration": {
                    "timeToLiveSecs": 7200
                }
            }
        },
        "interface1": {
            "properties.desired": {
                "SendData": true,
                "SendInterval": 5
            }
        },
        "interface2": {
            "properties.desired": {
                "SendData": true,
                "SendInterval": 5
            }
        }
    }
}