{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "modules": {
          "pump1": {
            "settings": {
              "image": "$CONTAINER_REGISTRY_SERVER/module-pump-win:1.5",
              "createOptions": {
                "HostConfig": {
                  "LogConfig": {
                    "Type": "json-file",
                    "Config": {
                      "max-size": "2m",
                      "max-file": "3"
                    }
                  }
                }
              }
            },
            "type": "docker",
            "version": "1.0",
            "env": {
              "ASSET": {
                "value": "$ASSET"
              },
              "DEVICE": {
                "value": "pump1"
              },
              "APPINSIGHTS_INSTRUMENTATIONKEY": {
                "value": "$APPINSIGHTS_INSTRUMENTATIONKEY"
              }
            },
            "status": "running",
            "restartPolicy": "always"
          },
          "pump2": {
            "settings": {
              "image": "$CONTAINER_REGISTRY_SERVER/module-pump-win:1.5",
              "createOptions": {
                "HostConfig": {
                  "LogConfig": {
                    "Type": "json-file",
                    "Config": {
                      "max-size": "2m",
                      "max-file": "3"
                    }
                  }
                }
              }
            },
            "type": "docker",
            "version": "1.0",
            "env": {
              "ASSET": {
                "value": "$ASSET"
              },
              "DEVICE": {
                "value": "pump2"
              },
              "APPINSIGHTS_INSTRUMENTATIONKEY": {
                "value": "$APPINSIGHTS_INSTRUMENTATIONKEY"
              }
            },
            "status": "running",
            "restartPolicy": "always"
          },
          "pump3": {
            "settings": {
              "image": "$CONTAINER_REGISTRY_SERVER/module-pump-win:1.5",
              "createOptions": {
                "HostConfig": {
                  "LogConfig": {
                    "Type": "json-file",
                    "Config": {
                      "max-size": "2m",
                      "max-file": "3"
                    }
                  }
                }
              }
            },
            "type": "docker",
            "version": "1.0",
            "env": {
              "ASSET": {
                "value": "$ASSET"
              },
              "DEVICE": {
                "value": "pump3"
              },
              "APPINSIGHTS_INSTRUMENTATIONKEY": {
                "value": "$APPINSIGHTS_INSTRUMENTATIONKEY"
              }
            },
            "status": "running",
            "restartPolicy": "always"
          }
        },
        "runtime": {
          "settings": {
            "minDockerVersion": "v1.25",
            "registryCredentials": {
              "privateRegistry": {
                "username": "$CONTAINER_REGISTRY_USERNAME",
                "password": "$CONTAINER_REGISTRY_PASSWORD",
                "address": "$CONTAINER_REGISTRY_SERVER"
              }
            }
          },
          "type": "docker"
        },
        "schemaVersion": "1.0",
        "systemModules": {
          "edgeAgent": {
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": ""
            },
            "type": "docker"
          },
          "edgeHub": {
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"443/tcp\":[{\"HostPort\":\"443\"}],\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}]}}}"
            },
            "type": "docker",
            "status": "running",
            "restartPolicy": "always"
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "routes": {
          "upstream": "FROM /messages/* INTO $upstream"
        },
        "schemaVersion": "1.0",
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "pump1": {
      "properties.desired": {
        "SendInterval": 10
      }
    },
    "pump2": {
      "properties.desired": {
        "SendInterval": 10
      }
    },
    "pump3": {
      "properties.desired": {
        "SendInterval": 10
      }
    }
  }
}